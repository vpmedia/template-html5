{
  "root": true,
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "flowtype"
  ],
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {}
    }
  }
}